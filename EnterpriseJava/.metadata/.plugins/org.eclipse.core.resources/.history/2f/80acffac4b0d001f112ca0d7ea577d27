package strutturedati;
import java.util.Comparator;
import java.util.TreeMap;
public class TreeMapComparatorEx {

	public static void main(String[] args) {
		Comparatore comparatore = new Comparatore();
		
		 TreeMap<Regione, Capoluogo> treeMap = new TreeMap<>(comparatore);
		 
		 treeMap.put(new Regione("Puglia", 1253),new Capoluogo("Bari", 25632));
}

}
